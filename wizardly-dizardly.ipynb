    "## 7. Prepare and Tokenize Dataset"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from datasets import Dataset\n",
    "from transformers import DataCollatorForLanguageModeling\n",
    "\n",
    "# Assume dataset has 'text' column from merge; prepare if needed\n",
    "def prepare_data(dataset):\n",
    "    # If no 'text', format from instruction/input/output\n",
    "    if 'text' not in dataset.column_names:\n",
    "        def format_text(ex):\n",
    "            prompt = f\"### Human: {ex.get('instruction', '')}\"\n",
    "            if ex.get('input', ''):\n",
    "                prompt += f\"\\n{ex['input']}\"\n",
    "            text = f\"{prompt}\\n### Assistant: {ex.get('output', '')}\"\n",
    "            return {'text': text.strip()}\n",
    "        dataset = dataset.map(format_text)\n",
    "    \n",
    "    # Filter valid texts\n",
    "    dataset = dataset.filter(lambda x: len(x['text']) > 50)\n",
    "    return dataset\n",
    "\n",
    "dataset = prepare_data(dataset)\n",
    "print(f\"üìù Prepared {len(dataset)} training examples\")\n",
    "\n",
    "def tokenize_function(examples):\n",
    "    return tokenizer(\n",
    "        examples[\"text\"],\n",
    "        truncation=True,\n",
    "        padding=True,\n",
    "        max_length=1024,\n",
    "        return_tensors=\"pt\"\n",
    "    )\n",
    "\n",
    "train_dataset = dataset.map(\n",
    "    tokenize_function,\n",
    "    batched=True,\n",
    "    remove_columns=dataset.column_names\n",
    ")\n",
    "\n",
    "print(\"‚úÖ Dataset tokenized\")\n",
    "\n",
    "# Space check after tokenization\n",
    "!df -h /"
   ]
