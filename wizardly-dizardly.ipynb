   "source": [
    "## 3. GCS Dataset Streaming Manager"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from datasets import load_dataset, Dataset\n",
    "import tempfile\n",
    "import json\n",
    "\n",
    "class GCSDatasetManager:\n",
    "    def __init__(self, client, bucket_name):\n",
    "        self.client = client\n",
    "        self.bucket = client.bucket(bucket_name)\n",
    "    \n",
    "    def stream_dataset_from_gcs(self, gcs_path, max_samples=500):\n",
    "        \"\"\"Stream dataset from GCS JSONL without full local download.\"\"\"\n",
    "        print(f\"üì• Streaming dataset from gs://{self.bucket.name}/{gcs_path}...\")\n",
    "        \n",
    "        # Use datasets' GCS loader with streaming\n",
    "        gcs_url = f\"gs://{self.bucket.name}/{gcs_path}\"\n",
    "        dataset_stream = load_dataset(\n",
    "            \"json\",\n",
    "            data_files=gcs_url,\n",
    "            split=\"train\",\n",
    "            streaming=True  # Line-by-line, no full load to disk\n",
    "        )\n",
    "        \n",
    "        # Collect subsample in memory (limit to max_samples for Kaggle)\n",
    "        data = []\n",
    "        for i, item in enumerate(dataset_stream):\n",
    "            if i >= max_samples:\n",
    "                break\n",
    "            data.append(item)\n",
    "        \n",
    "        # Convert to Dataset\n",
    "        dataset = Dataset.from_list(data)\n",
    "        print(f\"‚úÖ Streamed {len(dataset)} examples from GCS (limited to {max_samples})\")\n",
    "        return dataset\n",
    "    \n",
    "    def save_dataset_to_gcs(self, dataset, gcs_path):\n",
    "        \"\"\"Save dataset to GCS as JSONL (compact).\"\"\"\n",
    "        print(f\"üì§ Saving dataset to gs://{self.bucket.name}/{gcs_path}...\")\n",
    "        \n",
    "        # Save locally as JSONL (minimal footprint)\n",
    "        local_jsonl = \"/kaggle/working/temp_dataset.jsonl\"\n",
    "        dataset.to_json(local_jsonl, lines=True, orient=\"records\")\n",
    "        \n",
    "        # Upload\n",
    "        blob = self.bucket.blob(gcs_path)\n",
    "        blob.upload_from_filename(local_jsonl)\n",
    "        print(f\"‚úÖ Uploaded: gs://{self.bucket.name}/{gcs_path} (Size: {blob.size / (1024**2):.1f} MB)\")\n",
    "        \n",
    "        # Cleanup\n",
    "        os.remove(local_jsonl)\n",
    "        gc.collect()\n",
    "\n",
    "# Initialize dataset manager\n",
    "dataset_manager = GCSDatasetManager(client, BUCKET_NAME)"
   ]
