   "source": [
    "## 2. Setup GCS Authentication and Bucket"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import os\n",
    "import json\n",
    "import shutil\n",
    "import gc\n",
    "from kaggle_secrets import UserSecretsClient\n",
    "from google.cloud import storage\n",
    "import torch\n",
    "\n",
    "# GCS Configuration\n",
    "BUCKET_NAME = 'wizardlm-training-1759276927'\n",
    "DATASET_GCS_PATH = 'Datasets/merged_uncensored_alpaca.jsonl'  # From previous merge (or .zip if zipped)\n",
    "MODEL_OUTPUT_PATH = 'model_output/wizardlm_fine_tuned.zip'\n",
    "\n",
    "# Authenticate with GCS using Kaggle secret\n",
    "print(\"üîê Authenticating GCS...\")\n",
    "user_secrets = UserSecretsClient()\n",
    "service_account_json_str = user_secrets.get_secret(\"GCS_SERVICE_ACCOUNT\")\n",
    "\n",
    "if not service_account_json_str.strip():\n",
    "    raise ValueError(\"GCS_SERVICE_ACCOUNT secret is empty! Please add your GCS service account JSON as a Kaggle secret.\")\n",
    "\n",
    "# Validate JSON format\n",
    "try:\n",
    "    service_account_json = json.loads(service_account_json_str)\n",
    "except json.JSONDecodeError as e:\n",
    "    raise ValueError(f\"GCS_SERVICE_ACCOUNT secret is not valid JSON: {e}\")\n",
    "\n",
    "# Write the JSON key to a file\n",
    "service_account_path = \"/kaggle/working/gcs_service_account.json\"\n",
    "with open(service_account_path, \"w\") as f:\n",
    "    json.dump(service_account_json, f, indent=2)\n",
    "\n",
    "# Set the environment variable for Google Cloud authentication\n",
    "os.environ[\"GOOGLE_APPLICATION_CREDENTIALS\"] = service_account_path\n",
    "\n",
    "print(\"‚úÖ GCS service account set.\")\n",
    "\n",
    "# Initialize GCS client and bucket\n",
    "client = storage.Client()\n",
    "bucket = client.bucket(BUCKET_NAME)\n",
    "print(f\"‚úÖ Connected to GCS bucket: {BUCKET_NAME}\")\n",
    "\n",
    "# Verify dataset exists in GCS\n",
    "blob = bucket.blob(DATASET_GCS_PATH)\n",
    "if blob.exists():\n",
    "    print(f\"‚úÖ Dataset found: gs://{BUCKET_NAME}/{DATASET_GCS_PATH} (Size: {blob.size / (1024**2):.1f} MB)\")\n",
    "else:\n",
    "    raise ValueError(f\"Dataset not found in GCS: gs://{BUCKET_NAME}/{DATASET_GCS_PATH}. Run the merge notebook first!\")\n",
    "\n",
    "# Quick space check\n",
    "!df -h /"
   ]
